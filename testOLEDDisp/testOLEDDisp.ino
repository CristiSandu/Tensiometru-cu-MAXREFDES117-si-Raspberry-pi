#include "U8glib.h"
#include <dht.h>
#define DHT11_PIN 4
dht DHT;

U8GLIB_SH1106_128X64 u8g(13, 11, 10, 9, 8);  // D0=13, D1=11, CS=10, DC=9, Reset=8
void setup() {
   pinMode(DHT11_PIN,INPUT);
   
  u8g.firstPage();  
  do {
    u8g.setFont(u8g_font_helvB10);  
    u8g.drawStr(30, 10, "Welcome "); 
    u8g.drawStr(50, 30, "To ");
    u8g.drawStr(10, 50, "ElectronicsHob");
    u8g.drawStr(10, 60, "byists.com");
  } while( u8g.nextPage() );
  delay(5000);  
}
void loop() {
   int d = DHT.read11(DHT11_PIN);
  
  u8g.firstPage();  
  do {
    u8g.setFont(u8g_font_helvR12);  
    u8g.drawStr(0, 15, "Temp:");  
    u8g.setPrintPos(75, 15);
    u8g.print(DHT.temperature, 0);
    u8g.print((char)176);
    u8g.print("C");
    
    u8g.drawStr(0, 35, "Humi:");
    u8g.setPrintPos(75, 35);
    u8g.print(DHT.humidity, 0);
    u8g.print("%");
    
    u8g.drawStr(0, 55, "Hi:");
    u8g.setPrintPos(75, 55);
   
    u8g.print("%");
  } while( u8g.nextPage() );
  delay(5000);
}

